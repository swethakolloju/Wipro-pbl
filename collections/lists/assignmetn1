import java.util.ArrayList;

import java.util.*;

public class ArrayListMonth {
public static void main(String[] args)
{
	List<String> mn = new ArrayList<String>();
	
	mn.add("jan");
	mn.add("feb");
	mn.add("mar");
	mn.add("apr");
	mn.add("may");
	mn.add("jun");
	mn.add("jul");
	mn.add("aug");
	mn.add("sep");
	mn.add("nov");
	mn.add("oct");
	mn.add("dec");
	System.out.println(mn);
}
}
